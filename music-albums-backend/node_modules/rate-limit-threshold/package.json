{
  "name": "rate-limit-threshold",
  "version": "0.2.0",
  "description": "Rate limiter with a maximum number of calls per given period of time",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "repository": "https://github.com/Borewit/rate-limit-threshold",
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "license": "MIT",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Borewit/rate-limit/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.4",
    "@types/node": "^22.5.0",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "del-cli": "^5.1.0",
    "mocha": "^10.2.0",
    "remark-cli": "^12.0.0",
    "remark-preset-lint-recommended": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "clean": "del-cli 'lib/**/*.js' 'lib/**/*.js.map' 'lib/**/*.d.ts' 'test/**/*.js' 'test/**/*.js.map'",
    "compile-lib": "tsc -p lib",
    "compile-test": "tsc -p test",
    "compile": "yarn run compile-lib && yarn run compile-test",
    "lint-ts": "biome check",
    "lint-md": "remark -u preset-lint-recommended .",
    "lint": "yarn run lint-md && yarn run lint-ts",
    "test": "mocha",
    "build": "yarn run clean && npm run compile",
    "start": "yran yarn compile ** yarn run lint cover-test",
    "test-coverage": "c8 npm run test",
    "send-codacy": "c8 report --reporter=text-lcov | codacy-coverage"
  },
  "keywords": [
    "ratelimiter",
    "rate-limiting",
    "rate",
    "limit",
    "rolling",
    "limitting",
    "request",
    "restrict",
    "API"
  ],
  "dependencies": {
    "@biomejs/biome": "^1.8.3"
  },
  "packageManager": "yarn@4.4.1"
}
